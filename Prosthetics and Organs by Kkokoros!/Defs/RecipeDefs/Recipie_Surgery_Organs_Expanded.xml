    <RecipeDef ParentName="SurgerySimpleOrgans" MayRequire="Kkokoros.expandedorgans">
        <defName>KokuInstallArtificialSpleen</defName>
        <label>install artificial spleen</label>
        <description>Installs artificial spleen.</description>
        <jobString>Installing artificial spleen.</jobString>
        <ingredients>
           <li>
              <filter>
                 <categories>
                    <li>Medicine</li>
                 </categories>
              </filter>
              <count>3</count>
           </li>
           <li>
              <filter>
                 <thingDefs>
                    <li>KokuArtificialSpleen</li>
                 </thingDefs>
              </filter>
              <count>1</count>
           </li>
        </ingredients>
        <fixedIngredientFilter>
           <categories>
              <li>Medicine</li>
           </categories>
           <thingDefs>
              <li>KokuArtificialSpleen</li>
           </thingDefs>
        </fixedIngredientFilter>
        <appliedOnFixedBodyParts>
           <li>Spleen</li>
        </appliedOnFixedBodyParts>
        <addsHediff>KokuArtificialSpleen</addsHediff>
     </RecipeDef>

