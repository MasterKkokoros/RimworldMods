<Defs>
    <RecipeDef ParentName="SurgeryInstallBodyPartNaturalBase">
        <defName>InstallNaturalSpleen</defName>
        <label>install spleen</label>
        <description>Install a biological spleen.</description>
        <descriptionHyperlinks><ThingDef>Spleen</ThingDef></descriptionHyperlinks>
        <jobString>Installing spleen.</jobString>
        <ingredients>
          <li>
            <filter>
              <thingDefs>
                <li>Spleen</li>
              </thingDefs>
            </filter>
            <count>1</count>
          </li>
        </ingredients>
        <fixedIngredientFilter>
          <thingDefs>
            <li>Spleen</li>
          </thingDefs>
        </fixedIngredientFilter>
        <appliedOnFixedBodyParts>
          <li>Spleen</li>
        </appliedOnFixedBodyParts>
      </RecipeDef>
      
